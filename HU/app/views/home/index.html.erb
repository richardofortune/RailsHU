
<div class="well">
  <h1>The universe is the totality of everything that exists, has existed, or ever will exist.</h1>
  <small>Let that bake your noodle! </small>
</div>
<div class="container">
  <% @questions.each do |q| %>


  <div class="media">
    <div class="media-left">
      <a href="#">
        <img class="media-object" src="<%= q.gravatar %>" alt="Smiley face" height="64" width="64">
      </a>
    </div>

    <div class="media-body">
      <h4 class="media-heading"> <%= q.email %> asked:</h4>
      <div><%= time_ago_in_words q.created_at %> ago</div>
      <%= q.body %>
      <div>
        <a href="/questions/12" class="btn btn-success btn-xs">View Answers</a>
      </div>
    </div>
  <% end %>
  </div>

  

